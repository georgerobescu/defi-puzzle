{
    "name": "defi-puzzle",
    "version": "1.0.0",
    "description": "",
    "main": "truffle-config.js",
    "dependencies": {
        "camelcase": "^5.2.0",
        "classnames": "^2.2.6",
        "fbemitter": "^2.1.1",
        "pnp-webpack-plugin": "^1.5.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0"
    },
    "devDependencies": {
        "@babel/core": "7.5.5",
        "@babel/plugin-proposal-optional-chaining": "^7.6.0",
        "@svgr/webpack": "4.3.2",
        "babel-eslint": "10.0.2",
        "babel-jest": "^24.8.0",
        "babel-loader": "8.0.6",
        "babel-plugin-named-asset-import": "^0.3.3",
        "babel-preset-react-app": "^9.0.1",
        "case-sensitive-paths-webpack-plugin": "2.2.0",
        "css-loader": "2.1.1",
        "dotenv": "^8.2.0",
        "dotenv-expand": "4.2.0",
        "elliptic": "^6.5.0",
        "eslint": "^6.1.0",
        "eslint-config-react-app": "^5.0.1",
        "eslint-loader": "2.2.1",
        "eslint-plugin-flowtype": "3.13.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.14.3",
        "eslint-plugin-react-hooks": "^1.6.1",
        "file-loader": "3.0.1",
        "fs-extra": "7.0.1",
        "ganache-cli": "^6.6.0",
        "html-webpack-plugin": "4.0.0-beta.5",
        "identity-obj-proxy": "3.0.0",
        "is-wsl": "^1.1.0",
        "jest": "24.8.0",
        "jest-environment-jsdom-fourteen": "0.1.0",
        "jest-resolve": "24.8.0",
        "jest-watch-typeahead": "0.3.1",
        "mini-css-extract-plugin": "0.5.0",
        "openzeppelin-solidity": "2.3.0",
        "optimize-css-assets-webpack-plugin": "5.0.3",
        "postcss-flexbugs-fixes": "4.1.0",
        "postcss-loader": "3.0.0",
        "postcss-normalize": "7.0.1",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "4.0.1",
        "react-app-polyfill": "^1.0.4",
        "react-dev-utils": "^9.0.3",
        "resolve": "1.12.0",
        "resolve-url-loader": "3.1.0",
        "sass-loader": "7.2.0",
        "semver": "6.3.0",
        "solidity-coverage": "^0.6.4",
        "solidity-linked-list": "2.1.2",
        "solium": "^1.2.4",
        "solium-plugin-zeppelin": "^0.0.2",
        "style-loader": "1.0.0",
        "terser-webpack-plugin": "1.4.1",
        "truffle": "^5.0.33",
        "truffle-hdwallet-provider": "^1.0.17",
        "url-loader": "2.1.0",
        "webpack": "4.39.1",
        "webpack-dev-server": "3.2.1",
        "webpack-manifest-plugin": "2.0.4",
        "workbox-webpack-plugin": "4.3.1"
    },
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/topmonks/defi-puzzle.git"
    },
    "scripts": {
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js",
        "solium": "./node_modules/.bin/solium -d contracts",
        "truffle": "./node_modules/.bin/truffle",
        "test-contracts": "./node_modules/.bin/truffle test",
        "coverage": "./node_modules/.bin/solidity-coverage",
        "ganache": "./node_modules/.bin/ganache-cli -m 'gadget luxury river width frost salmon input defense embark card banner raccoon'",
        "migrate": "./node_modules/.bin/truffle migrate --network ganache_cli --reset"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "bugs": {
        "url": "https://github.com/topmonks/defi-puzzle/issues"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ],
        "plugins": [
            "@babel/plugin-proposal-optional-chaining"
        ]
    }
}
